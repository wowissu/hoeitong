{% extends 'overall/html.twig' %}

{% block styles %}
    <link rel="stylesheet" href="css/normalize.css">
    <link rel="stylesheet" href="css/font-awesome/css/font-awesome.css">
    <link rel="stylesheet" href="css/froala/froala_editor.pkgd.min.css">
    <link rel="stylesheet" href="css/froala/froala_style.min.css">
{% endblock %}

{% block Title %}Hoei Tong{% endblock %}

{% block meta %}
    <meta http-equiv="set-cookie" content="csrf_token={{ csrf_token }}; SameSite=Strict;">
    <meta name="csrf-token" content="{{ csrf_token }}">
    <meta name="robots" content="noindex">
    <meta name="googlebot" content="noindex">
{% endblock %}

{% block Body %}
    <div id="app"></div>
{% endblock %}

{% block scripts %}
    <script src="js/jquery/jquery.min.js"></script>
    <script src="dist/build.js"></script>
{% endblock %}